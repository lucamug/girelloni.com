mixin flag-small(country)
	.flag-small
		a(href='https://kozuredeyoroppa.blogspot.de/search/label/#{country.en}' title='#{country.ja}#{japaneseSpace}#{country.en}')
			span= country.en
			+flag(country.en)

mixin sectionFlag(country)
	.cp-postlink.sectionFlag
		a(href='https://kozuredeyoroppa.blogspot.de/search/label/#{country.en}' title='#{country.ja}#{japaneseSpace}#{country.en}')
			span.wrap
				span.image
					+flag(country.en)
			span.text
				span.title #{country.ja}#{japaneseSpace}#{country.en}

mixin sectionPost(post)
	.cp-postlink.sectionPost
		a(href=post.href title=post.titleEn)
			span.wrap
				span.image
					img(src=post.imageUrl)
			span.text
				span.title= post.title

doctype html
head
	meta(name="version" content=pkg.version)
	meta(charset='UTF-8')
	meta(name='viewport', content='width=device-width, initial-scale=1.0')
	title Blog
	block livereload
		script(src='http://localhost:35729/livereload.js')
	block style
	script.
		document.documentElement.className = document.documentElement.className.replace(/no-js/, "yes-js");
		document.documentElement.className = document.documentElement.className + (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ? " yes-touch" : " no-touch");
		document.documentElement.className = document.documentElement.className + (/iPhone/i.test(navigator.userAgent) ? " yes-iPhone" : " no-iPhone");
	block script
body
	h1 cp-flagsmall-container 
	.cp-flagsmall-container
		each country in countries
			+flag-small(country)

	h1 cp-postlinks-container 
	.cp-postlinks-container
		each post in posts.reverse()
			+sectionPost(post)

	h1 Large flags 
	.cp-postlinks-container
		each country in countries
			+sectionFlag(country)
	h1 Bremerhaven 


	block script-bottom
